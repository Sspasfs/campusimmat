<para><computeroutput>$<link linkend="_report_8contract_8php_1a77b973d137fb33212e018b042df6e3e7">supported_algs</link><computeroutput>[static]</computeroutput></computeroutput></para><emphasis role="strong">Valeur initiale :</emphasis><programlisting linenumbering="unnumbered">=&#32;[
&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="stringliteral">&apos;ES384&apos;</emphasis>&#32;=&gt;&#32;[<emphasis role="stringliteral">&apos;openssl&apos;</emphasis>,&#32;<emphasis role="stringliteral">&apos;SHA384&apos;</emphasis>],
&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="stringliteral">&apos;ES256&apos;</emphasis>&#32;=&gt;&#32;[<emphasis role="stringliteral">&apos;openssl&apos;</emphasis>,&#32;<emphasis role="stringliteral">&apos;SHA256&apos;</emphasis>],
&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="stringliteral">&apos;ES256K&apos;</emphasis>&#32;=&gt;&#32;[<emphasis role="stringliteral">&apos;openssl&apos;</emphasis>,&#32;<emphasis role="stringliteral">&apos;SHA256&apos;</emphasis>],
&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="stringliteral">&apos;HS256&apos;</emphasis>&#32;=&gt;&#32;[<emphasis role="stringliteral">&apos;hash_hmac&apos;</emphasis>,&#32;<emphasis role="stringliteral">&apos;SHA256&apos;</emphasis>],
&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="stringliteral">&apos;HS384&apos;</emphasis>&#32;=&gt;&#32;[<emphasis role="stringliteral">&apos;hash_hmac&apos;</emphasis>,&#32;<emphasis role="stringliteral">&apos;SHA384&apos;</emphasis>],
&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="stringliteral">&apos;HS512&apos;</emphasis>&#32;=&gt;&#32;[<emphasis role="stringliteral">&apos;hash_hmac&apos;</emphasis>,&#32;<emphasis role="stringliteral">&apos;SHA512&apos;</emphasis>],
&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="stringliteral">&apos;RS256&apos;</emphasis>&#32;=&gt;&#32;[<emphasis role="stringliteral">&apos;openssl&apos;</emphasis>,&#32;<emphasis role="stringliteral">&apos;SHA256&apos;</emphasis>],
