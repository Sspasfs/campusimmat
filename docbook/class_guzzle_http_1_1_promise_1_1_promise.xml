<entry>bool</entry><entry>$unwrap</entry><entry></entry>
                            </row>
                        </tbody>
                        </tgroup>
                    </table>
                    </para>
                </formalpara>
                <formalpara><title>Renvoie</title>

<para>mixed</para>
</formalpara>

