            <imageobject>
                <imagedata width="50%" align="center" valign="middle" scalefit="0" fileref="class_laminas_1_1_validator_1_1_in_array_a72063d2e922edda5321b1970297eb0c3_cgraph.svg"></imagedata>
            </imageobject>
        </mediaobject>
    </informalfigure>
</para>
</section>
