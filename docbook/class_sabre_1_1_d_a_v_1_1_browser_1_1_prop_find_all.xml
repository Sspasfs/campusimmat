                            <row>
<entry>string</entry><entry>$propertyName</entry><entry>
<para></para>
</entry>
                            </row>
                            <row>
<entry>mixed</entry><entry>$valueOrCallBack</entry><entry>
<para></para>
</entry>
                            </row>
                        </tbody>
                        </tgroup>
