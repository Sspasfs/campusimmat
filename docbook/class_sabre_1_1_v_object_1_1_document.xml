                        <colspec colwidth="1*"/>
                        <colspec colwidth="4*"/>
                        <tbody>
                            <row>
<entry>string</entry><entry>$name</entry><entry>
<para></para>
</entry>
                            </row>
                            <row>
<entry>mixed</entry><entry>$value</entry><entry>
<para></para>
</entry>
                            </row>
                            <row>
<entry>array</entry><entry>$parameters</entry><entry>
<para></para>
</entry>
                            </row>
                            <row>
<entry>string</entry><entry>$valueType</entry><entry>
<para>Force a specific valuetype, such as URI or TEXT</para>
</entry>
