<indexterm><primary>Text</primary><secondary>__construct</secondary></indexterm>
<para><computeroutput>__construct (<link linkend="_class_sabre_1_1_v_object_1_1_component">Component</link> $root,  $name,  $value = <computeroutput>null</computeroutput>
