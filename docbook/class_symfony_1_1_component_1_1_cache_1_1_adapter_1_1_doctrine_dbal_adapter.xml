                            <row>
<entry>InvalidArgumentException</entry><entry>
<para>When namespace contains invalid characters </para>
</entry>
                            </row>
                        </tbody>
                        </tgroup>
                    </table>
                    </para>
                </formalpara>
                </para>
</section>
</section>
